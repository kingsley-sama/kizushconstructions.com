{
	"name": "multicoder",
	"version": "0.0.25",
	"displayName": "Multicoder - Claude Sonnet in Copilot chat",
	"description": "Add Claude 3.5 Sonnet to Github Copilot chat (BYOK)",
	"publisher": "multicoder",
	"license": "SEE LICENSE IN LICENSE.txt",
	"main": "./out/extension.js",
	"author": "Multicoder AI",
	"icon": "resources/multicoder.png",
	"homepage": "https://multicoder.dev",
	"repository": {
		"type": "git",
		"url": "https://github.com/multicoder-ai/vscode-release"
	},
	"bugs": {
		"url": "https://github.com/multicoder-ai/vscode-release/issues"
	},
	"engines": {
		"vscode": "^1.94.0"
	},
	"extensionDependencies": [
		"github.copilot-chat"
	],
	"categories": [
		"AI",
		"Chat",
		"Machine Learning",
		"Programming Languages"
	],
	"keywords": [
		"claude",
		"anthropic",
		"deepseek",
		"deepseekcoder",
		"copilot",
		"chatgpt",
		"gpt",
		"openapi",
		"byok",
		"ai",
		"assistant",
		"coding",
		"automation"
	],
	"activationEvents": [
		"onFileSystem:multicoder"
	],
	"contributes": {
		"chatParticipants": [
			{
				"id": "claude",
				"fullName": "Claude (multicoder)",
				"name": "claude",
				"description": "Ask claude to write code",
				"isSticky": true,
				"when": "config.multicoder.enabledModels =~ /claude/",
				"commands": [
					{
						"name": "config",
						"description": "Change/review claude configuration"
					}
				]
			},
			{
				"id": "deepseek",
				"fullName": "DeepSeek (multicoder)",
				"name": "deepseek",
				"description": "Ask deepseek to write code",
				"isSticky": true,
				"when": "config.multicoder.enabledModels =~ /deepseek/",
				"commands": [
					{
						"name": "config",
						"description": "Change/review deepseek configuration"
					}
				]
			},
			{
				"id": "gpt4o",
				"fullName": "GPT4o (multicoder)",
				"name": "gpt4o",
				"description": "Ask gpt4o to write code",
				"isSticky": true,
				"when": "config.multicoder.enabledModels =~ /gpt4o/",
				"commands": [
					{
						"name": "config",
						"description": "Change/review gpt4o configuration"
					}
				]
			}
		],
		"commands": [
			{
				"command": "multicoder.storeAnthropicKey",
				"title": "Multicoder: configure Anthropic API key"
			},
			{
				"command": "multicoder.action",
				"title": "Multicoder: Generic Command (internal)"
			},
			{
				"command": "multicoder.work",
				"title": "Multicoder: Work experiments (internal)"
			}
		],
		"configuration": {
			"title": "Multicoder",
			"properties": {
				"multicoder.enabledModels": {
					"type": "array",
					"items": {
						"type": "string",
						"enum": [
							"claude",
							"deepseek",
							"gpt4o"
						]
					},
					"uniqueItems": true,
					"default": [
						"claude"
					],
					"markdownDescription": "Defines which models are visible as chat participants"
				},
				"multicoder.modelProvider": {
					"type": "string",
					"enum": [
						"default",
						"openrouter"
					],
					"enumItemLabels": [
						"Default",
						"Open Router"
					],
					"default": "default",
					"markdownDescription": "Defines the model provider to use"
				}
			}
		}
	},
	"scripts": {
		"build": "node esbuild.js",
		"vscode:prepublish": "node esbuild.js --production",
		"publish": "vsce publish patch",
		"test": "vscode-test"
	},
	"dependencies": {
		"@anthropic-ai/sdk": "^0.29",
		"@vscode/extension-telemetry": "^0.9",
		"diff": "^5",
		"graphology": "^0.25",
		"graphology-communities-louvain": "^2",
		"graphology-gexf": "^0.13",
		"graphology-metrics": "^2",
		"graphology-shortest-path": "^2",
		"htm": "^3",
		"ignore": "^5",
		"nanoid": "^5",
		"openai": "^4",
		"zod": "^3",
		"zod-to-json-schema": "^3"
	},
	"devDependencies": {
		"@vscode/test-cli": "^0.0.10",
		"@vscode/test-electron": "^2",
		"@vscode/vsce": "^3",
		"@types/diff": "^5",
		"@types/node": "^20",
		"@types/vscode": "1.94.0",
		"esbuild": "^0.23",
		"tsx": "^4"
	},
	"prettier": {
		"tabWidth": 4,
		"printWidth": 120,
		"singleQuote": true,
		"trailingComma": "none",
		"endOfLine": "lf"
	},
	"__metadata": {
		"id": "f7b35b8f-44e2-4053-ad78-e9323e64deb6",
		"publisherId": "6c31d842-6c01-4f77-86ce-f6f31df982e1",
		"publisherDisplayName": "Multicoder",
		"targetPlatform": "undefined",
		"isApplicationScoped": false,
		"isPreReleaseVersion": false,
		"hasPreReleaseVersion": false,
		"installedTimestamp": 1733411007777,
		"pinned": false,
		"preRelease": false,
		"source": "gallery"
	}
}